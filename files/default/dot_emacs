;;; .init.el --- init config emacs

;;; Commentary:
;;;; Author: kakakikikeke
;;;; Version: 1.0

;;; Code:
; load each init scripts
(defun load-config (name)
  "Load config file from ~/.emacs.d/site-lisp/NAME/init.el"
  (let ((path (expand-file-name (concat "site-lisp/" name "/init.el") user-emacs-directory)))
    (when (file-exists-p path)
      (load-file path))))
(load-config "package")
(load-config "ruby")
(load-config "python")
(load-config "java")
(load-config "lisp")
(load-config "lua")
(load-config "helm")
(load-config "tool")
(load-config "common_func")
(load-config "common_clipboard")
(load-config "common_ui")
(load-config "common_keybind")
;;; .emacs ends here
